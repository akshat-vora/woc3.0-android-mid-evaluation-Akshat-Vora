<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimeTable">
        <activity
            android:name=".daily_profs"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".professors_2"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Professors"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_sat"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_fri"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_thu"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_wed"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_tuesday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".schedule_mon"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>